<web-app>
	<display-name>FB-Botmill demo app</display-name>

	<servlet>
		<servlet-name>botmill-demo-conversation</servlet-name>
		<servlet-class>co.aurasphere.botmill.fb.FbBotMillServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>botmill-demo-conversation</servlet-name>
